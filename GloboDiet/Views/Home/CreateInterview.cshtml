@model GloboDiet.Models.Interview

<h1>CreateInterview</h1>
<h4>Interview</h4>
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreateInterview">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Timestamp" class="control-label"></label>
                <input asp-for="Timestamp" class="form-control" />
                <span asp-validation-for="Timestamp" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Number" class="control-label"></label>
                <input asp-for="Number" class="form-control" />
                <span asp-validation-for="Number" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ReferenceDate" class="control-label"></label>
                <input asp-for="ReferenceDate" class="form-control" />
                <span asp-validation-for="ReferenceDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



