@model GloboDiet.ViewModels.InterviewerCreateEdit
<h1>Create</h1>

<h4>Interviewer</h4>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Interviewer.Code" class="control-label"></label>
                <input asp-for="Interviewer.Code" class="form-control" />
                <span asp-validation-for="Interviewer.Code" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Interviewer.GivenName" class="control-label"></label>
                <input asp-for="Interviewer.GivenName" class="form-control" />
                <span asp-validation-for="Interviewer.GivenName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Interviewer.Name" class="control-label"></label>
                <input asp-for="Interviewer.Name" class="form-control" />
                <span asp-validation-for="Interviewer.Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

